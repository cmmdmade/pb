          "_customHTML": "<section class=\"features1\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' &amp;&amp; bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"10\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Image height\" name=\"imgheight\" min=\"1\" max=\"28\" step=\"1\" value=\"13\">\n        <input type=\"range\" inline=\"\" title=\"Cards Amount\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"(showTitle || showText)\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Icon\" name=\"showicon\" checked=\"\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showUpTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Column Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Column Text\" name=\"showText\" checked=\"\">\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"\" parallax=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#000000\" selected=\"\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"row h-100 justify-content-center align-items-center\">\n\n            <div class=\"col-12 col-lg-3 align-center\">\n                <span mbr-icon=\"\" class=\"mbr-iconfont mbri-cursor-click mb-5 mr-md-5 mr-0\" mbr-if=\"showicon\"></span>\n            </div>\n            <div class=\"col-12 col-lg-9\">\n                <p class=\"mbr-text mbr-fonts-style display-5 justify-content-center align-items-center logo-text mb-5 mt-5 mt-md-5 pr-md-5 px-md-0 mr-md-5 mx-md-0\" data-app-selector=\".logo-text\" mbr-theme-style=\"display-5\" mbr-if=\"showUpTitle\">Web traffic marketing solutions available to any business within the United States. Schedule a consultation to <a href=\"https://consult.proximitybillboards.solutions\" class=\"text-primary\" target=\"_blank\">learn more</a>.</p>\n            </div>\n        </div>\n        <div class=\"row align-items-top align-top align-items-tp align-top\">\n            <div class=\"col-lg-3 col-12\"></div>\n            <div class=\"card col-12 col-lg-2 align-top\" mbr-class=\"{'col-lg-3': cardsAmount == '3','col-md-2': cardsAmount == '2', 'col-sm-6': cardsAmount == '4', 'col-md-6': cardsAmount == '4', 'col-md-4': cardsAmount == '3'}\">\n                <div class=\"card-box\">\n                    <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .card-img\">MAILING ADDRESS</h5>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Proximity Billboards<br>4062 Peachtree Road NE&nbsp;A529<br>Atlanta, Georgia 30319 USA</p>\n                </div>\n            </div>\n\n            <div class=\"card col-12 col-lg-2 align-top\" mbr-if=\"cardsAmount > 1\" mbr-class=\"{'col-md-2': cardsAmount == '2',\n                             'col-lg-3': cardsAmount == '3', 'col-md-4': cardsAmount == '3', 'col-sm-6': cardsAmount == '4', 'col-md-6': cardsAmount == '4'}\">\n                <div class=\"card-box\">\n                    <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .card-img\">SOLUTIONS</h5>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text\"><a href=\"presence.html\" class=\"text-primary\">Business Presence</a></p>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text\"><a href=\"https://proximitybillboards.adtrafficexpert.com\" class=\"text-primary\">Paid Web Traffic Solutions</a></p>\n                </div>\n            </div>\n\n            <div class=\"card col-12 col-lg-2 align-top\" mbr-if=\"cardsAmount > 2\" mbr-class=\"{'col-lg-3': cardsAmount == '3',\n                'col-md-4': cardsAmount == '3', 'col-sm-6': cardsAmount == '4', 'col-md-6': cardsAmount == '4'}\">\n                <div class=\"card-box\">\n                    <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .card-img\">RESOURCES</h5>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text\"><a href=\"https://google.com/business/?ppsrc=GPDA2\" class=\"text-primary\" target=\"_blank\">Google Business Profile Help</a></p>\n                </div>\n            </div>\n\n            <div class=\"card col-12 col-md-6 col-lg-2 align-top\" mbr-if=\"cardsAmount > 3\" mbr-class=\"{'col-lg-2': cardsAmount == '2' || cardsAmount == '3' || cardsAmount == '4', 'col-sm-6': cardsAmount == '4'}\">\n                <div class=\"card-box\">\n                    <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title, .card-img\">LINKS</h5>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text\"><a href=\"pb-list.html\" class=\"text-primary\">JOIN MAILING LIST</a></p>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text\"><a href=\"antispam.html\" class=\"text-primary\">AntiSpam Policy</a></p>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text\"><a href=\"earnings.html\" class=\"text-primary\">Earnings Disclaimer</a></p>\n                </div>\n            </div>\n        </div>\n    </div>\n\n</section>",
